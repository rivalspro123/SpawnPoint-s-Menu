-- Scripts:

local function TQCEX_fake_script() -- SpinAroundPlayerUI.Handler 
	local script = Instance.new('LocalScript', SpinAroundPlayerUI)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local Workspace = game:GetService("Workspace")
	local Camera = Workspace:FindFirstChild("Camera") or Workspace:FindFirstChildWhichIsA("Camera")
	
	local localPlayer = Players.LocalPlayer
	local gui = script.Parent
	
	local existingGui = localPlayer:FindFirstChild("PlayerGui") and localPlayer.PlayerGui:FindFirstChild(gui.Name)
	if existingGui and existingGui ~= gui then
		script.Parent:Destroy()
		return
	end
	
	if not gui:IsDescendantOf(localPlayer.PlayerGui) then
		gui.Parent = localPlayer.PlayerGui
	end
	
	local radius = 6
	local speedRange = {10,30}
	local verticalRange = {-4,4}
	local pulseCooldown = 0.05
	local pulseDistance = 4
	local buttonCooldown = 1
	
	_G.orbitState = _G.orbitState or {
		running = false,
		targetName = "",
	}
	
	local currentAngle = 0
	local spinSpeed = math.random(speedRange[1],speedRange[2])
	local verticalOffset = math.random(verticalRange[1]*10,verticalRange[2]*10)/10
	local direction = math.random(0,1)==0 and -1 or 1
	local pulseTime = 0
	local orbitConnection
	local randomizerThread
	local buttonLocked = false
	
	local startStopButton
	local usernameTextBox
	
	local function acquireUI()
		local frame = gui:WaitForChild("MainFrame")
		local main = frame:WaitForChild("Main")
		startStopButton = main:WaitForChild("Button"):WaitForChild("StartStopButton")
		usernameTextBox = main:WaitForChild("Username"):WaitForChild("UsernameTextbox")
	end
	
	local function setButtonState(isRunning)
		if not startStopButton then return end
		if isRunning then
			startStopButton.Text = "Stop"
			startStopButton.BackgroundColor3 = Color3.fromRGB(139,0,0)
		else
			startStopButton.Text = "Start"
			startStopButton.BackgroundColor3 = Color3.fromRGB(0,100,0)
		end
	end
	
	local function randomizeSpin()
		spinSpeed = math.random(speedRange[1],speedRange[2])
		verticalOffset = math.random(verticalRange[1]*10,verticalRange[2]*10)/10
		direction = math.random(0,1)==0 and -1 or 1
	end
	
	local function findTarget(name)
		if not name or name == "" then return nil end
		local lower = string.lower(name)
		for _, plr in ipairs(Players:GetPlayers()) do
			if string.lower(plr.Name) == lower or string.lower(plr.DisplayName) == lower then
				local char = plr.Character
				if char and char:FindFirstChild("Humanoid") and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Head") then
					return char
				end
			end
		end
		for _, model in ipairs(Workspace:GetDescendants()) do
			if model:IsA("Model") and string.lower(model.Name) == lower then
				if model:FindFirstChild("Humanoid") and model:FindFirstChild("HumanoidRootPart") and model:FindFirstChild("Head") then
					return model
				end
			end
		end
		return nil
	end
	
	local function getMyCharacter()
		return localPlayer.Character
	end
	
	local function updateOrbit(target, dt)
		local myChar = getMyCharacter()
		if not target or not myChar then return end
	
		local targetHead = target:FindFirstChild("Head")
		local myHRP = myChar:FindFirstChild("HumanoidRootPart")
		if not targetHead or not myHRP then return end
	
		currentAngle += spinSpeed * dt * direction
		pulseTime += dt
		local pulse = 1
		if pulseTime >= pulseCooldown then
			pulseTime = 0
			pulse = (math.random() > 0.5) and 1 or -1
		end
	
		local dynamicRadius = radius + (pulse * pulseDistance)
		local offsetX = math.cos(currentAngle) * dynamicRadius
		local offsetZ = math.sin(currentAngle) * dynamicRadius
	
		myHRP.CFrame = CFrame.new(
			targetHead.Position + Vector3.new(offsetX, verticalOffset, offsetZ),
			targetHead.Position
		)
	
		if Camera then
			local targetHumanoid = target:FindFirstChild("Humanoid")
			if targetHumanoid then
				Camera.CameraType = Enum.CameraType.Custom
				Camera.CameraSubject = targetHumanoid
			end
		end
	end
	
	local function stopOrbit()
		if orbitConnection then
			orbitConnection:Disconnect()
			orbitConnection = nil
		end
		if randomizerThread then
			task.cancel(randomizerThread)
			randomizerThread = nil
		end
	
		if Camera then
			local myChar = getMyCharacter()
			if myChar and myChar:FindFirstChild("Humanoid") then
				Camera.CameraType = Enum.CameraType.Custom
				Camera.CameraSubject = myChar:FindFirstChild("Humanoid")
			end
		end
	end
	
	local function startOrbitLoop()
		stopOrbit()
		orbitConnection = RunService.Heartbeat:Connect(function(dt)
			if not _G.orbitState.running then return end
			local target = findTarget(_G.orbitState.targetName)
			if target then
				updateOrbit(target, dt)
			end
		end)
	
		randomizerThread = task.spawn(function()
			while _G.orbitState.running do
				task.wait(math.random(3,10)/10)
				if _G.orbitState.running then randomizeSpin() end
			end
		end)
	end
	
	local function restoreUI()
		if usernameTextBox then
			usernameTextBox.Text = _G.orbitState.targetName
		end
		setButtonState(_G.orbitState.running)
	end
	
	local function toggle()
		if buttonLocked then return end
		buttonLocked = true
		task.delay(buttonCooldown,function() buttonLocked=false end)
	
		if _G.orbitState.running then
			_G.orbitState.running = false
			stopOrbit()
			setButtonState(false)
		else
			local name = (usernameTextBox.Text or ""):gsub("^%s*(.-)%s*$","%1")
			local target = findTarget(name)
			if not target then
				if startStopButton then
					startStopButton.Text = "Player Not Found"
					startStopButton.BackgroundColor3 = Color3.fromRGB(255,255,255)
				end
				task.delay(1, restoreUI)
				return
			end
			if target == getMyCharacter() then
				if startStopButton then
					startStopButton.Text = "Can't target self"
					startStopButton.BackgroundColor3 = Color3.fromRGB(255,255,255)
				end
				task.delay(1, restoreUI)
				return
			end
	
			_G.orbitState.running = true
			_G.orbitState.targetName = name
			startOrbitLoop()
			setButtonState(true)
		end
	end
	
	local function init()
		acquireUI()
		restoreUI()
		startStopButton.Activated:Connect(toggle)
		if _G.orbitState.running and _G.orbitState.targetName ~= "" then
			startOrbitLoop()
		end
	end
	
	init()
	
	localPlayer.CharacterAdded:Connect(function()
		stopOrbit()
		task.wait(0.25)
		init()
	end)
end
coroutine.wrap(TQCEX_fake_script)()
local function FUXJW_fake_script() -- MainFrame.Dragify 
	local script = Instance.new('LocalScript', MainFrame)

	local player = game.Players.LocalPlayer
	
	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		dragSpeed = 0.15
		dragInput = nil
		dragStart = nil
		dragPos = nil
		function updateInput(input)
			Delta = input.Position - dragStart
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.15), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	dragify(script.Parent)
	
end
coroutine.wrap(FUXJW_fake_script)()
